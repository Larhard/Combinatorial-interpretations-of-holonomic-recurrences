graph {
	size="4,4"
    0 [label="\\(\\n\\)" shape=none]
    1 [label="\\(\\n\\)" shape=none]
    2 [label="\\(\\n\\)" shape=none]
    3 [label="\\(\\n\\)" shape=none]
    4 [label="\\(\\n\\)" shape=none]
    5 [label="\\(\\n\\)" shape=none]

    00 [label="\\(\\n\\)" shape=none style=invis]
    01 [label="\\(\\n\\)" shape=none style=invis]
    02 [label="\\(\\n\\)" shape=none style=invis]
    03 [label="\\(\\n\\)" shape=none style=invis]
    04 [label="\\(\\n\\)" shape=none style=invis]
    05 [label="\\(\\n\\)" shape=none style=invis]
    06 [label="\\(\\n\\)" shape=none style=invis]

    0 -- 1
    0 -- 2
    1 -- 3
    2 -- 4
    2 -- 5
    1 -- 00 [color=lightgreen]
    3 -- 01 [color=lightgreen]
    3 -- 02 [color=lightgreen]
    4 -- 03 [color=lightgreen]
    4 -- 04 [color=lightgreen]
    5 -- 05 [color=lightgreen]
    5 -- 06 [color=lightgreen]
}
